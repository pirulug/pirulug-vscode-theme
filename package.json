{
    "name": "pirulug-theme",
    "displayName": "Pirulug Theme",
    "description": "A minimal theme that comes in dark and light.",
    "version": "0.0.1",
    "publisher": "Pirulug",
    "repository": {
		"type": "git",
		"url": "https://github.com/pirulug/pirulug-theme.git"
	},
    "engines": {
        "vscode": "^1.66.0"
    },
    "categories": [
        "Themes"
    ],
    "contributes": {
        "themes": [
            {
                "label": "Pirulug Dark",
                "uiTheme": "vs-dark",
                "path": "./themes/pirulug-dark.json"
            },
            {
                "label": "Pirulug Light",
                "uiTheme": "vs",
                "path": "./themes/pirulug-light.json"
            }
        ]
    },
    "galleryBanner": {
		"color": "#ff0055",
		"theme": "dark"
	},
    "scripts": {
        "build": "node src/dark/index.js && node src/light/index.js"
    }
}